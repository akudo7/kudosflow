{
  "name": "kudosflow",
  "displayName": "kudosflow",
  "description": "kudosflow",
  "version": "2.0.0",
  "publisher": "akirakudo",
  "license": "MIT",
  "author": {
    "name": "Akira Kudo",
    "url": "https://www.linkedin.com/in/akira-kudo-4b04163/"
  },
  "bugs": {
    "url": "https://github.com/akudo7/kudos-gpt/issues"
  },
  "repository": {
    "url": "https://github.com/akudo7/kudos-gpt/blob/main/README.md"
  },
  "homepage": "https://github.com/akudo7/kudos-gpt/blob/main/README.md",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": []
    },
    "commands": [
      {
        "command": "kudosflow.openWorkflowEditor",
        "title": "Open Workflow Editor",
        "category": "Kudosflow"
      },
      {
        "command": "kudosflow.createNewWorkflow",
        "title": "Create New Workflow Here",
        "category": "Kudosflow"
      }
    ],
    "menus": {
      "view/title": [],
      "view/item/context": [],
      "explorer/context": [
        {
          "command": "kudosflow.openWorkflowEditor",
          "when": "resourceExtname == .json",
          "group": "navigation"
        },
        {
          "command": "kudosflow.createNewWorkflow",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "install:all": "yarn install && cd webview-ui && yarn install",
    "clean": "rm -rf out .tsbuildinfo webview-ui/build",
    "clean:vsix": "rm -f *.vsix *.vsix.backup",
    "build": "yarn run build:webview && yarn run build:extension",
    "build:extension": "rm -f .tsbuildinfo && tsc -p ./",
    "build:webview": "cd webview-ui && yarn run build",
    "watch": "tsc -watch -p ./",
    "watch:webview": "cd webview-ui && yarn run start",
    "obfuscate": "javascript-obfuscator out --output out --compact true --control-flow-flattening true",
    "prebuild:dev": "yarn run build",
    "prebuild:prod": "yarn run build && yarn run obfuscate",
    "package:dev": "yarn run prebuild:dev && vsce package",
    "package:prod": "yarn run prebuild:prod && vsce package",
    "package": "yarn run package:dev",
    "pretest": "yarn run build:extension && yarn run lint",
    "lint": "eslint src --ext ts",
    "audit": "yarn audit",
    "audit:fix": "yarn upgrade-interactive --latest",
    "audit:report": "yarn audit --json > audit-report.json",
    "security:check": "yarn audit && yarn outdated",
    "security:full": "yarn audit && yarn outdated"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.39.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.5",
    "@kudos/scene-graph-manager": "file:../../TypeScript/SceneGraphManager",
    "ajv-formats": "^3.0.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1"
  },
  "resolutions": {
    "ajv": "^8.17.1",
    "qs": "^6.14.1",
    "brace-expansion": "^2.0.2",
    "@eslint/plugin-kit": "^0.3.4",
    "js-yaml": "^4.1.1",
    "glob": "^10.5.0"
  }
}
